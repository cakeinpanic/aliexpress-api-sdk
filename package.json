{
  "name": "aliexpress-api-sdk",
  "version": "1.1.0",
  "description": "Node.js SDK for AliExpress API",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "build:cjs-to-esm": "node scripts/convert.js index.js index.mjs && node scripts/convert.js lib/base.js lib/base.mjs",
    "build:esm-to-cjs": "node scripts/convert.js index.mjs index.js && node scripts/convert.js lib/base.mjs lib/base.js",
    "build": "npm run build:cjs-to-esm",
    "prepublishOnly": "npm test && npm run build"
  },
  "keywords": [
    "aliexpress",
    "api",
    "sdk",
    "ecommerce",
    "dropshipping"
  ],
  "author": {
    "name": "cakeinpanic"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cakeinpanic/aliexpress-api-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/cakeinpanic/aliexpress-api-sdk/issues"
  },
  "homepage": "https://github.com/cakeinpanic/aliexpress-api-sdk#readme",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts",
    "lib/",
    "scripts/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "crypto": "^1.0.1",
    "form-data": "^4.0.0",
    "os": "^0.1.2"
  }
}
